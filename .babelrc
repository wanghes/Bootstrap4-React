{
  "presets": [
        ["env",{
        	"targets":{
        		"uglify": true
        	}
        }],
        "stage-0",
        "react"
    ],
    "plugins": [
        "react-hot-loader/babel"
    ] ,
    "comments": false,
    "env": {
	    "development": {
		     "plugins": [["react-transform", {
		        "transforms": [{
		          	"transform": "react-transform-hmr",
		          	"imports": ["react"],
		          	"locals": ["module"]
		        }]
		    }]]
	    }
    }
}
